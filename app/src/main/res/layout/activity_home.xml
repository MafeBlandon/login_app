<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".HomeActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="199dp"
        android:layout_height="80dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.528"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/labial" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="Compra en linea"
        android:textColor="#0E0606"
        android:textSize="15sp"

        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Espacio Seguro"
        android:textColor="#3A77DD"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/textView"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView"

        />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/background_main_man"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="1.0" />

    <TableLayout
        android:id="@+id/tableLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="16dp"
                android:orientation="vertical">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="85dp"
                    android:layout_height="73dp">

                    <ImageView

                        android:layout_width="107dp"
                        android:layout_height="89dp"
                        android:backgroundTint="#17E8E3E3"
                        app:srcCompat="?android:attr/textSelectHandleRight"
                        app:tint="#0B0A0A" />

                    <ImageView

                        android:layout_width="78dp"
                        android:layout_height="69dp"
                        app:srcCompat="@drawable/baseline_shopping_cart_24"
                        app:tint="#EFECEC"

                        />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Carrito"
                    android:textAlignment="center"
                    android:textColor="#F1EBEB" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="16dp"
                android:orientation="vertical">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="85dp"
                    android:layout_height="73dp">

                    <ImageView

                        android:layout_width="107dp"
                        android:layout_height="89dp"
                        android:backgroundTint="#17E8E3E3"
                        app:srcCompat="?android:attr/textSelectHandleRight"
                        app:tint="#070707" />

                    <ImageView

                        android:layout_width="78dp"
                        android:layout_height="69dp"
                        app:srcCompat="@drawable/baseline_local_shipping_24"
                        app:tint="#F7F8F6"

                        />

                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/guideline2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        app:layout_constraintGuide_begin="20dp" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <TextView

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Envios"
                    android:textAlignment="center"
                    android:textColor="#F1EBEB" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="16dp"
                android:orientation="vertical">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="85dp"
                    android:layout_height="73dp">

                    <ImageView

                        android:layout_width="107dp"
                        android:layout_height="89dp"
                        android:backgroundTint="#17E8E3E3"
                        app:srcCompat="?android:attr/textSelectHandleRight"
                        app:tint="#0B0B0B" />

                    <ImageView

                        android:layout_width="78dp"
                        android:layout_height="69dp"
                        app:srcCompat="@drawable/baseline_favorite_24"
                        app:tint="#FDFDFD"

                        />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <TextView

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Favoritos"
                    android:textAlignment="center"
                    android:textColor="#F1EBEB" />
            </LinearLayout>
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="16dp"
                android:orientation="vertical">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="85dp"
                    android:layout_height="73dp">

                    <ImageView
                        android:layout_width="107dp"
                        android:layout_height="89dp"
                        android:backgroundTint="#17E8E3E3"
                        app:srcCompat="?android:attr/textSelectHandleRight"
                        app:tint="#040303" />

                    <ImageView

                        android:layout_width="78dp"
                        android:layout_height="69dp"
                        app:srcCompat="@drawable/baseline_person_pin_circle_24"
                        app:tint="@color/white"

                        />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <TextView

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Ubicacion"
                    android:textAlignment="center"
                    android:textColor="#F1EBEB" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="16dp"
                android:orientation="vertical">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="85dp"
                    android:layout_height="73dp">

                    <ImageView
                        android:layout_width="107dp"
                        android:layout_height="89dp"
                        android:backgroundTint="#17E8E3E3"
                        app:srcCompat="?android:attr/textSelectHandleRight"
                        app:tint="#090909" />

                    <ImageView

                        android:layout_width="78dp"
                        android:layout_height="69dp"
                        app:srcCompat="@drawable/baseline_weekend_24"
                        app:tint="#FDFDFC"

                        />

                    <androidx.constraintlayout.widget.Guideline

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        app:layout_constraintGuide_begin="20dp" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <TextView

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Muebles "
                    android:textAlignment="center"
                    android:textColor="#F1EBEB" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="16dp"
                android:orientation="vertical">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="85dp"
                    android:layout_height="73dp">

                    <ImageView

                        android:layout_width="107dp"
                        android:layout_height="89dp"
                        android:backgroundTint="#17E8E3E3"
                        app:srcCompat="?android:attr/textSelectHandleRight"
                        app:tint="#070707" />

                    <ImageView

                        android:layout_width="78dp"
                        android:layout_height="69dp"
                        app:srcCompat="@drawable/baseline_warehouse_24"
                        app:tint="#F5F8F5" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <TextView

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="+ Para mi hogar"
                    android:textAlignment="center"
                    android:textColor="#F1EBEB" />
            </LinearLayout>
        </TableRow>

    </TableLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="170dp"
        android:layout_margin="16dp"
        android:background="@drawable/banner_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout2">

        <Button
            android:id="@+id/buttoningresar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="20dp"
            android:text="Ingresar"
            android:backgroundTint="#901E027A"
            app:tint="@color/white"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5"
            app:layout_constraintTop_toTopOf="@+id/textView5"

            />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="213dp"
            android:layout_height="158dp"
            android:layout_margin="0dp"
            android:layout_marginEnd="71dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"

            tools:srcCompat="@drawable/hogar" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="30dp"
            android:layout_marginStart="16dp"
            android:text="Se parte de nosotros "
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>